 alias gcm="git checkout master"
 alias gs="git status"
 alias gf="git fetch"
 alias glog="git log --oneline --graph"
 alias gp="git pull"
 alias gck="git checkout"
 alias gcmm="git commit -m"
 alias gap="git add -p"
 alias gflog="git log"
 alias gaall="git add ."
